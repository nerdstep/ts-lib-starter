{
  "name": "ts-lib-starter",
  "version": "0.2.3",
  "description": "TypeScript library starter project",
  "license": "MIT",
  "repository": "nerdstep/ts-lib-starter",
  "source": "src/index.ts",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Justin Williams",
    "email": "justinwilliams42@gmail.com"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "typescript",
    "library",
    "starter",
    "boilerplate",
    "esbuild",
    "eslint",
    "jest",
    "swc",
    "typedoc"
  ],
  "scripts": {
    "start": "tsup-node --watch",
    "build": "tsup-node",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "lint": "yarn lint:src && yarn ts:check && yarn format:check",
    "lint:src": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --no-cache --coverage",
    "ts:check": "tsc --noEmit",
    "ts:emit": "tsc --emitDeclarationOnly --outDir dist",
    "docs": "typedoc --out docs src/index.ts",
    "release": "standard-version",
    "release:tag": "git push --follow-tags origin master"
  },
  "peerDependencies": {},
  "dependencies": {},
  "devDependencies": {
    "@swc/core": "^1.3.32",
    "@swc/jest": "^0.2.24",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "prettier": "^2.8.3",
    "standard-version": "^9.5.0",
    "tsup": "^6.6.0",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.5"
  }
}
